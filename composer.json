{
	"name": "laravel-plus/extension",
	"type": "framework-extension",
	"description": "Laravel Extension Pack",
	"license": "MIT",
	"keywords": ["Laravel", "Laravel4", "addon", "architecture", "facade"],
	"homepage": "http://laravel.tokyo",
	"authors": [
		{
			"name": "Fumio Furukawa",
			"email": "fumio@jumilla.me"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"laravel/framework": "~5.0"
	},
	"autoload": {
		"psr-4": {
			"LaravelPlus\\Extension\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
